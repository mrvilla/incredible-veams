---
title: Build Better Prototypes with Veams
menuLink: Home
sortOrder: 01
---

{{#extend "lyt-default"}}
	{{#content "main"}}
		<h2 class="myHeading">Veams components!</h2>

        {{#wrapWith "u-grid-row"}}
            {{#wrapWith "u-grid-col" colClasses="is-col-mobile-s-12 is-col-tablet-s-12 is-col-tablet-l-12 is-col-desktop-l-12"}}

                {{#with slider-bp.variations.autoplay}}
                    <h2 class="myHeading">Carousel Image <span>{{docs.variationName}}</span></h2>
                    {{! wrapWith START: Slider }}
                    {{#wrapWith "c-slider" settings=this.settings content=this.content}}
                    {{! WrapWith START: Slider List }}

                        {{#wrapWith "c-slider__list"}}
                            {{#times 3}}
                                {{#wrapWith "c-slider__item"}}
                                    <img class="image" src="https://placehold.it/1920x800" alt="test">
                                {{/wrapWith}}
                            {{/times}}
                        {{/wrapWith}}
                    {{! WrapWith END: Slider List }}
                    {{/wrapWith}}
                {{! wrapWith END: Slider}}
                {{/with}}

            {{/wrapWith}}
        {{/wrapWith}}


        {{#wrapWith "u-grid-row"}}
            {{#wrapWith "u-grid-col" colClasses="u-grid-col is-col-mobile-s-12 is-col-mobile-l-8"}}

                {{#with slide-fox-bp.variations.default}}

                    <h2 class="myHeading">Slide Fox <span>{{docs.variationName}}</span></h2>
                    <h3 class="myHeading">{{content.heading}}</h3>
                    <p>{{content.contentText}}</p>

                    {{#wrapWith "c-slide-fox" settings=this.settings}}
                        {{#with @root.article-bp.variations.slideFox}}
                            {{#wrapWith "c-article" settings=this.settings content=this.content}}
                                {{content.articleBody.articleText}}
                            {{/wrapWith}}
                        {{/with}}
                    {{/wrapWith}}
                {{/with}}

            {{/wrapWith}}
        {{/wrapWith}}


        {{#wrapWith "u-grid-row"}}
            {{#wrapWith "u-grid-col" colClasses="u-grid-col is-col-mobile-s-12 is-col-mobile-l-6"}}

                {{#with slider-bp.variations.multiple}}
                    <h2 class="myHeading">Carousel Image <span>{{docs.variationName}}</span></h2>
                    {{! wrapWith START: Slider }}
                    {{#wrapWith "c-slider" settings=this.settings content=this.content}}
                    {{! WrapWith START: Slider List }}
                        {{#wrapWith "c-slider__list"}}
                            {{#times 9}}
                                {{#wrapWith "c-slider__item"}}
                                    <img class="image" src="https://placehold.it/800x400" alt="test">
                                {{/wrapWith}}
                            {{/times}}
                        {{/wrapWith}}
                    {{! WrapWith END: Slider List }}
                    {{/wrapWith}}
                {{! wrapWith END: Slider}}
                {{/with}}

            {{/wrapWith}}
            {{#wrapWith "u-grid-col" colClasses="u-grid-col is-col-mobile-s-12 is-col-mobile-l-6"}}

                {{#with slider-bp.variations.infinite}}
                    <h2 class="myHeading">Carousel Image <span>{{docs.variationName}}</span></h2>
                    {{! wrapWith START: Slider }}
                    {{#wrapWith "c-slider" settings=this.settings content=this.content}}
                    {{! WrapWith START: Slider List }}
                        {{#wrapWith "c-slider__list"}}
                            {{#times 2}}
                                {{#wrapWith "c-slider__item"}}
                                    <img class="image" src="https://placehold.it/800x400" alt="test">
                                {{/wrapWith}}
                            {{/times}}
                        {{/wrapWith}}
                    {{! WrapWith END: Slider List }}
                    {{/wrapWith}}
                {{! wrapWith END: Slider}}
                {{/with}}

            {{/wrapWith}}
        {{/wrapWith}}



        {{#wrapWith "u-grid-row"}}
            {{#wrapWith "u-grid-col" colClasses="u-grid-col is-col-mobile-s-12 is-col-mobile-l-6"}}

                {{#with accordion-bp.variations.simple}}
                    <h2 class="myHeading">Accordion <span>{{docs.variationName}}</span></h2>

                    {{! WrapWith START: Accordion }}
                    {{#wrapWith "c-accordion" settings=this.settings}}
                    {{! WrapWith START: Item }}
                        {{#each content.items}}
                            {{#wrapWith "c-accordion__item" accItemId=this.itemId accButton=itemBtnText}}
                                {{itemText}}
                            {{/wrapWith}}
                        {{/each}}
                    {{! WrapWith END: Item }}
                    {{/wrapWith}}
                {{! WrapWith END: Accordion }}
                {{/with}}

            {{/wrapWith}}
            {{#wrapWith "u-grid-col" colClasses="u-grid-col is-col-mobile-s-12 is-col-mobile-l-6"}}

                {{#with accordion-bp.variations.singleOpen}}
                    <h2 class="myHeading">Accordion <span>{{docs.variationName}}</span></h2>
                    {{! WrapWith START: Accordion }}
                    {{#wrapWith "c-accordion" settings=this.settings}}
                    {{! WrapWith START: Item }}
                        {{#each content.items}}
                            {{#wrapWith "c-accordion__item" accItemId=this.itemId accButton=itemBtnText}}
